					"kind": 4096,
					"kindString": "Call signature",
					"flags": {},
					"comment": {
						"summary": [
							{
								"kind": "text",
								"text": "Outputs a heading relative to the current section scope.\n\nThis will typically output "
							},
							{
								"kind": "code",
								"text": "`<h1...6>`"
							},
							{
								"kind": "text",
								"text": " elements; however, this will revert to showing\nheadings using "
							},
							{
								"kind": "code",
								"text": "`<div>`"
							},
							{
								"kind": "text",
								"text": " elements with the accompanying "
							},
							{
								"kind": "code",
								"text": "`role`"
							},
							{
								"kind": "text",
								"text": " and "
							},
							{
								"kind": "code",
								"text": "`aria-level`"
							},
							{
								"kind": "text",
								"text": "\nattributes for headings deeper than "
							},
							{
								"kind": "code",
								"text": "`<h6>`"
							},
							{
								"kind": "text",
								"text": ". Applications should keep heading levels\nas shallow as possible.\n\nHeading levels are automatically computed from the scope of the current section. If\nnecessary the "
							},
							{
								"kind": "code",
								"text": "`level`"
							},
							{
								"kind": "text",
								"text": " property can be used to define a deeper level that is relative\nto the current section scope."
							}
						],
